pipeline {
  agent any
  stages {
    stage('Init'){
      steps {
        echo 'Testing Process starts'
        echo 'Api Testing Process'
        echo 'Functional Testing Process '
        echo 'Performance Testing Process'
        echo 'Testing Process Ends'
      }
    }

    stage('Build'){
      steps {
        echo 'Building process Starts...'
        echo 'Building process Ends'
      }
    }

    stage('Deploy'){
      steps {
        echo 'code Deployed'
      }
    }
}
}
